#activity1
#Crate a vector
family_df <- c( "litzy", "gabi", "celeste", "lola", "dumbo")

#Create a dataframe

myfamily <- data.frame( name = family_df, 
                        age = c(22, 23, 28, 5, 8),
                        gender = c("female", "female", "female", "female", "male"),
                        commute = c(40, 0,0, 20, 25))
#Calcualte average age and commute                        
mean(myfamily$age)
mean(myfamily$commute)
#Calculate the age of the youngest person

min(myfamily$age)
#calculate the longest commute time of a person

max(myfamily$commute)
